EXTRADEFS = \
	-DWINBASEAPI= \
	-DWINADVAPI= \
	-DWINUSERAPI= \
	-DWINSHLWAPI= \
	-DWINE_ASAN=$(SANITIZE) \
	-DASAN_SHADOW_OFFSET=$(ASAN_SHADOW_OFFSET)
MODULE    = kernelbase.dll
IMPORTLIB = kernelbase
IMPORTS   = uuid ntdll

EXTRADLLFLAGS = -nodefaultlibs -nostartfiles
i386_EXTRADLLFLAGS = -Wl,--image-base,0x7b000000
x86_64_EXTRADLLFLAGS = -Wl,--image-base,0x174000000

SOURCES = \
	console.c \
	debug.c \
	file.c \
	kernelbase.rc \
	loader.c \
	locale.c \
	main.c \
	memory.c \
	path.c \
	process.c \
	registry.c \
	security.c \
	string.c \
	sync.c \
	thread.c \
	version.c \
	volume.c \
	winerror.mc
